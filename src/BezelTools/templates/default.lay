<mamelayout version="2">

  <element name="bezel">
    <image file="{{game}}.png" />
  </element>

  <view name="Cab Artwork ">
    <screen index="0">
      <bounds x="{{x}}" y="{{y}}" width="{{width}}" height="{{height}}" />
    </screen>

    <bezel element="bezel">
      <bounds x="0" y="0" width="{{width_res}}" height="{{height_res}}" />
    </bezel>
  </view>

</mamelayout>
